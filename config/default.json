{
  "detection": {
    "method": "ocr",
    "confidence_threshold": 0.8,
    "button_text": [
      "Continue",
      "继续",
      "Continuar", 
      "Continuer",
      "Weiter",
      "続行",
      "계속",
      "Продолжить"
    ],
    "image_templates": [],
    "ocr_language": "eng",
    "preprocessing": {
      "grayscale": true,
      "contrast_enhancement": 1.2,
      "noise_reduction": false
    },
    "cache_duration": 30,
    "scale_factor": 1.0
  },
  "automation": {
    "click_delay": 0.1,
    "retry_attempts": 3,
    "retry_delay": 1.0,
    "safety_pause": 2.0,
    "preserve_focus": true,
    "window_activation_delay": 0.1,
    "click_offset": {
      "x": 0,
      "y": 0
    }
  },
  "filtering": {
    "include_workspaces": [],
    "exclude_workspaces": [
      "test",
      "tmp",
      ".vscode",
      "node_modules"
    ],
    "min_window_age": 5,
    "require_visible": true,
    "max_windows": 10
  },
  "safety": {
    "user_activity_timeout": 5.0,
    "confirmation_required": false,
    "disable_on_fullscreen": true
  },
  "logging": {
    "level": "INFO",
    "console_output": true,
    "file_path": "logs/automation.log",
    "max_file_size_mb": 10,
    "backup_count": 5,
    "detailed_timing": false
  },
  "gui": {
    "theme": "dark",
    "language": "en",
    "show_tips": true
  },
  "performance": {
    "max_screenshot_size": [1920, 1080],
    "image_compression_quality": 85,
    "parallel_processing": true,
    "max_concurrent_windows": 3
  }
}
